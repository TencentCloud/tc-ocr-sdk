.seal-field {
  display: flex;
  width: 100%;
  background-color: #FFFFFF;
  border-radius: 0;
  box-sizing: border-box;
}
.seal-field__textarea,
.seal-field__input {
  display: flex;
  font-size: 16px;
  color: #000000;
  width: 100%;
  border-radius: inherit;
  align-items: center;
}
.seal-field__textarea.is-disabled,
.seal-field__input.is-disabled {
  color: #BBBBBB;
  /* background-color: #E4E5E9; */
}
.seal-field__textarea.is-disabled::after,
.seal-field__input.is-disabled::after {
  border-color: #E4E5E9;
}
.seal-field__textarea.is-focus::after,
.seal-field__input.is-focus::after {
  border-color: #006EFF;
}
.seal-field__textarea.is-error,
.seal-field__input.is-error {
  color: #FF584C;
}
.seal-field__textarea.is-error::after,
.seal-field__input.is-error::after {
  border-color: #FF584C;
}
.seal-field__textarea {
  position: relative;
  padding: 12px;
}
.seal-field__textarea-item {
  display: block;
  width: 100%;
  height: auto;
  min-height: 96px;
  max-height: 180px;
}
.seal-field__textarea-item.size-auto {
  min-height: 0;
  max-height: none;
}
.seal-field__textarea-item.is-count {
  padding-bottom: 30px;
}
.seal-field__textarea-counter {
  position: absolute;
  font-size: 14px;
  color: #888888;
  right: 12px;
  bottom: 12px;
}
.seal-field__textarea-current {
  color: #000000;
}
.seal-field__input {
  padding: 12px;
}
.seal-field__input-item {
  display: block;
  width: 100%;
  min-height: 24px;
  height: 24px;
  line-height: inherit;
  text-overflow: clip;
  overflow: hidden;
  white-space: nowrap;
}
.seal-field__placeholder {
  color: #E4E5E9;
}
.seal-field__placeholder--error {
  color: #FF584C;
}
